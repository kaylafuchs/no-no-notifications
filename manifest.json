{
    "name": "no no notifications",
    "version": "0.1.0",
    "description": "hide browser tab notifications because that shit is distracting.",
    "permissions": [
        "tabs"
    ],
    "optional_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": ["no-no.js"],
        "persistent": true
    },
    "manifest_version": 2
}
